<h1>Users#index</h1>

<table>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= link_to "Details", user_path(user) %> </td>
    <td><%= link_to "Edit", edit_user_path(user) %></td>
  </tr>
  <% end %>
  <tr>
    <td><%= link_to "New User", new_user_path %></td>
  </tr>
</table>