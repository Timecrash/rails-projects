<h1 align="center">Create Event</h1>

<%= form_for @event do |f| %>
  <table align="center">
    <tr>
      <td><%= f.label :description %></td>
      <td><%= f.text_field :description %></td>
    </tr>
    <tr>
      <td><%= f.label :location %></td>
      <td><%= f.text_field :location %></td>
    </tr>
    <tr>
      <td><%= f.submit "Sign Up" %></td>
    </tr>
  </table>
<% end %>